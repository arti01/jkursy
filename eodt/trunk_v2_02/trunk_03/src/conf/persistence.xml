<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.0" xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd">
  <persistence-unit name="eodtPU" transaction-type="JTA">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <class>pl.eod.encje.Dzial</class>
    <class>pl.eod.encje.Hasla</class>
    <class>pl.eod.encje.Struktura</class>
    <class>pl.eod.encje.UserRoles</class>
    <class>pl.eod.encje.Uzytkownik</class>
    <class>pl.eod.encje.WnHistoria</class>
    <class>pl.eod.encje.WnRodzaje</class>
    <class>pl.eod.encje.WnStatusy</class>
    <class>pl.eod.encje.WnUrlop</class>
    <class>pl.eod.encje.Config</class>
    <class>pl.eod.encje.KomKolejka</class>
    <class>pl.eod.encje.MenuLinki</class>
    <class>pl.eod.encje.WnLimity</class>
    <class>pl.eod.encje.Spolki</class>
    <class>pl.eod2.encje.DcRodzajGrupa</class>
    <class>pl.eod2.encje.DcPlik</class>
    <class>pl.eod2.encje.DcDokument</class>
    <class>pl.eod2.encje.DcZrodlo</class>
    <class>pl.eod2.encje.DcRodzaj</class>
    <class>pl.eod2.encje.DcProjekt</class>
    <class>pl.eod2.encje.DcTypFlow</class>
    <class>pl.eod2.encje.DcAkceptKroki</class>
    <class>pl.eod2.encje.DcAkceptTypKroku</class>
    <class>pl.eod2.encje.DcKontrahenci</class>
    <class>pl.eod2.encje.DcDokDoWiadCel</class>
    <class>pl.eod2.encje.DcDokDoWiadomosci</class>
    <exclude-unlisted-classes>false</exclude-unlisted-classes>
    <shared-cache-mode>ALL</shared-cache-mode>
    <properties>
      <jta-data-source>java:comp/env/jdbc/eod_v2</jta-data-source>
      <!-- automatyczne zakladanie tabel
      <property name="eclipselink.ddl-generation" value="create-tables"/>
      -->
      
      <property name="javax.persistence.validation.group.pre-update" value="true"/>
      <property name="eclipselink.logging.parameters" value="true"/>
      <property name="eclipselink.logging.level.sql" value="finest"/>
      <property name="eclipselink.logging.level.cache" value="finest"/>
    </properties>
  </persistence-unit>
</persistence>
