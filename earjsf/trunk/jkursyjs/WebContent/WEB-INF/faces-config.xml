<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
	version="2.0">
	<application>
	<message-bundle>Messages</message-bundle>
	<locale-config>
		<default-locale>pl_PL</default-locale>
	</locale-config>
	</application>
	
	<managed-bean>
		<managed-bean-name>adminIndexAc</managed-bean-name>
		<managed-bean-class>org.arti01.admin.IndexAc</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>adminUsersAc</managed-bean-name>
		<managed-bean-class>org.arti01.admin.UsersAc</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>adminKursyAc</managed-bean-name>
		<managed-bean-class>org.arti01.admin.KursyAc</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>adminStatyczneAc</managed-bean-name>
		<managed-bean-class>org.arti01.admin.StatyczneAc</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>

	<navigation-rule>
		<display-name>/all/wszystko</display-name>
		<from-view-id>/all/*</from-view-id>
		<navigation-case>
			<from-outcome>index</from-outcome>
			<to-view-id>/all/index.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>indexAdm</from-outcome>
			<to-view-id>/admin/index.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>indexWyklad</from-outcome>
			<to-view-id>/wykladowca/index.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>indexAbsolw</from-outcome>
			<to-view-id>/absolwent/index.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>indexKursant</from-outcome>
			<to-view-id>/kursant/index.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>*Lista</from-outcome>
			<to-view-id>/all/*Lista.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>*Detale</from-outcome>
			<to-view-id>/all/*Detale.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

<navigation-rule>
		<display-name>/admin/wszystko</display-name>
		<from-view-id>/admin/*</from-view-id>
		<navigation-case>
			<from-outcome>indexAll</from-outcome>
			<to-view-id>/all/index.xhtml</to-view-id>
			<redirect/>
		</navigation-case>
		<navigation-case>
			<from-outcome>indexAdm</from-outcome>
			<to-view-id>/admin/index.xhtml</to-view-id>
			<redirect/>
		</navigation-case>
		<navigation-case>
			<from-outcome>*Lista</from-outcome>
			<to-view-id>/admin/*Lista.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>*Form</from-outcome>
			<to-view-id>/admin/*Form.xhtml</to-view-id>
		</navigation-case>
</navigation-rule>

<navigation-rule>
		<display-name>/wykladowca/wszystko</display-name>
		<from-view-id>/wykladowca/*</from-view-id>
		<navigation-case>
			<from-outcome>indexAll</from-outcome>
			<to-view-id>/all/index.xhtml</to-view-id>
			<redirect/>
		</navigation-case>
		<navigation-case>
			<from-outcome>indexAdm</from-outcome>
			<to-view-id>/wykladowca/index.xhtml</to-view-id>
			<redirect/>
		</navigation-case>
		<navigation-case>
			<from-outcome>*Lista</from-outcome>
			<to-view-id>/wykladowca/*Lista.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>*Form</from-outcome>
			<to-view-id>/wykladowca/*Form.xhtml</to-view-id>
		</navigation-case>
</navigation-rule>

<navigation-rule>
		<display-name>/kursant/wszystko</display-name>
		<from-view-id>/kursant/*</from-view-id>
		<navigation-case>
			<from-outcome>indexAll</from-outcome>
			<to-view-id>/all/index.xhtml</to-view-id>
			<redirect/>
		</navigation-case>
</navigation-rule>

<navigation-rule>
		<display-name>/absolwent/wszystko</display-name>
		<from-view-id>/absolwent/*</from-view-id>
		<navigation-case>
			<from-outcome>indexAll</from-outcome>
			<to-view-id>/all/index.xhtml</to-view-id>
			<redirect/>
		</navigation-case>
</navigation-rule>

	<navigation-rule>
		<display-name>/test/wszystko</display-name>
		<from-view-id>/test/*</from-view-id>
		<navigation-case>
			<from-outcome>index</from-outcome>
			<to-view-id>/test/index.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>kursy</from-outcome>
			<to-view-id>/test/kursyLista.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
</faces-config>
