<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.0" xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd">
	<persistence-unit name="jkursyJPA">
		<jta-data-source>jdbc/slow</jta-data-source>
		<class>org.arti01.obiekty.UserRole</class>
		<class>org.arti01.obiekty.User</class>
		<class>org.arti01.obiekty.Statyczne</class>
		<class>org.arti01.obiekty.Role</class>
		<class>org.arti01.obiekty.KursyUser</class>
		<class>org.arti01.obiekty.Kursy</class>
		<properties>
			<property name="eclipselink.logging.level" value="INFO"/>
			<property name="eclipselink.logging.exceptions" value="true"/>
			<property name="eclipselink.jdbc.cache-statements" value="false"/>
			<property name="eclipselink.cache.type.default" value="NONE"/>
		</properties>
	</persistence-unit>
</persistence>
